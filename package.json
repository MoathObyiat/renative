{
    "name": "renative",
    "title": "ReNative",
    "version": "0.21.15",
    "codename": "Magic Beans",
    "keywords": [
        "react",
        "react-native",
        "template",
        "starter",
        "boilerplate",
        "kit",
        "native",
        "swift",
        "cocoapods",
